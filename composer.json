{
    "name": "neos/event-sourcing-demo",
    "type": "project",
    "description": "Demo of the Symfony bridge to integrate Event Sourcing and CQRS pattern in your Symfony framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Kurf√ºrst",
            "email": "sebastian.kurfuerst@sandstorm.de"
        },
        {
            "name": "Timo Nussbaum",
            "email": "timo.nussbaum@gmx.de"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Inchie/Neos.EventSourcingSymfonyBridge"
        }
    ],
    "require": {
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "neos/error-messages": "^7.3",
        "neos/event-sourcing": "dev-master",
        "neos/event-sourcing-symfony-bridge": "dev-main",
        "neos/utility-objecthandling": "^7.3",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.4.*",
        "symfony/messenger": "5.4.*",
        "symfony/process": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/yaml": "5.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2",
        "squizlabs/php_codesniffer": "^3.5",
        "slevomat/coding-standard": "^6.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Neos\\EventSourcing\\SymfonyBridge\\Tests\\": "vendor/neos/event-sourcing-symfony-bridge/tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
