{
    "name": "neos/event-sourcing-demo",
    "type": "project",
    "description": "Demo of the Neos Event Sourcing and CQRS pattern in your Symfony framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Kurf√ºrst",
            "email": "sebastian.kurfuerst@sandstorm.de"
        },
        {
            "name": "Timo Nussbaum",
            "email": "timo.nussbaum@gmx.de"
        }
    ],
    "require": {
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.1.1",
        "doctrine/orm": "^2.7",
        "neos/error-messages": "^7.3",
        "neos/eventstore": "dev-main",
        "neos/eventstore-doctrineadapter": "dev-main",
        "neos/utility-objecthandling": "^7.3",
        "phpstan/phpstan": "^1.8",
        "sensio/framework-extra-bundle": "*",
        "symfony/asset": "6.1.*",
        "symfony/console": "6.1.*",
        "symfony/doctrine-messenger": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "6.1.*",
        "symfony/messenger": "6.1.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/process": "6.1.*",
        "symfony/twig-bundle": "6.1.*",
        "symfony/yaml": "6.1.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "rector/rector": "^0.13.8",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "6.1.*",
        "symfony/css-selector": "6.1.*",
        "symfony/phpunit-bridge": "^6.1",
        "symplify/easy-coding-standard": "^11.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/flex": true
        },
        "process-timeout": 0
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    }
}
